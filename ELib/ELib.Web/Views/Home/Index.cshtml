@{
    Layout = null;
        }
<html>
<head>
    <title>
        Elib - best lib ever.
    </title>
    <link rel="stylesheet" href="../Content/materialize.css" />
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">    <style>
        .maincontent {
            min-height: 73%;
        }

        .section {
            padding: 0px;
        }

        footer.page-footer {
            margin-top: 0px;
            padding-top: 0px;
        }
    </style>
</head>
<body>
    <header>
        <nav>
            <div class="nav-wrapper container">
                <a id="logo-container" href="#" class="brand-logo"> ELib</a>
                <ul id="nav-mobile" class="right hide-on-med-and-down">
                    <li>
                        <form>
                            <div class="input-field">
                                <input id="search" type="search" required>
                                <label for="search"><i class="material-icons">search</i></label>
                            </div>
                        </form>
                    </li>
                    <li><a href="/Help"> API </a></li>
                    <li><a href="#"><i class="material-icons right">account_box</i>Profile</a></li>
                    <li><a href="#"><i class="material-icons right">exit_to_app</i>Sign out</a></li>
                </ul>
            </div>
        </nav>
    </header>
    <main>
        <div class="section indigo lighten-5">
            <div class="container white" ng-app="ELib">
                <div class="row">
                    <div class="col s3">
                        <ul class="collapsible" data-collapsible="expanadable">
                            <li>
                                <div class="collapsible-header"><i class="material-icons">mood</i>Genres</div>
                                <div class="collapsible-body"><p>Body</p></div>
                            </li>
                            <li>
                                <div class="collapsible-header"><i class="material-icons">person</i>Authors</div>
                                <div class="collapsible-body"><p>Body</p></div>
                            </li>
                        </ul>
                    </div>
                    <div class="col s9 maincontent">
                        <ng-view></ng-view>
                        <!--Main Content-->
                        <!--Content placeholder-->
                    </div>
                </div>
            </div>
        </div>
    </main>
    <footer class="page-footer grey">
        <div class="container">
            <div class="row">
                <div class="col l6 s12">
                    <h4 class="white-text">Footer</h4>
                    <p class="grey-text text-lighten-4">Some not very important information.</p>
                </div>
                <div class="col l4 offset-l2 s12">
                    <h5 class="white-text">Links</h5>
                    <ul>
                        <li><a class="grey-text text-lighten-3" href="#!">Link 1</a></li>
                        <li><a class="grey-text text-lighten-3" href="#!">Link 2</a></li>
                        <li><a class="grey-text text-lighten-3" href="#!">Link 3</a></li>
                        <li><a class="grey-text text-lighten-3" href="#!">Link 4</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </footer>
    <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
    <script src="../Scripts/materialize.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.4/angular.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.4/angular-route.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.4/angular-resource.min.js"></script>
    @Scripts.Render("~/Views/Home/Elib.js")
    @Scripts.Render("~/Views/Home/config.js")
    @Scripts.Render("~/Views/Home/common/DataServiceFactory.js")
    @Scripts.Render("~/Views/Home/book/BooksController.js")
</body>
</html>